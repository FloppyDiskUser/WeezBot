<Controls:MetroWindow
        x:Class="WeezBot.MainWindow" Closing="Window_Closed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                      xmlns:gif="clr-namespace:XamlAnimatedGif;assembly=XamlAnimatedGif"
                      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d"
                      xmlns:m="clr-namespace:Microsoft.Maps.MapControl.WPF;assembly=Microsoft.Maps.MapControl.WPF"
                      xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
                      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                      xmlns:local="clr-namespace:WeezBot"
                      xmlns:util="clr-namespace:WeezBot.Utils"
                      xmlns:model="clr-namespace:WeezBot.Model"
                      xmlns:rapi="clr-namespace:PokemonGo.RocketAPI.Enums;assembly=Pokemon.Go.Rocket.API"
        Title="WeezBot[Pokemon Go Bot]" TitleForeground="black" TitleCharacterCasing="Normal" Height="700" Width="1360" FontFamily="segoe ui light"  
                      BorderBrush="{DynamicResource AccentColorBrush}"
                      TextElement.Foreground="{DynamicResource MaterialDesignBody}"
                      BorderThickness="1" Background="{DynamicResource MaterialDesignPaper}"
                      WindowStartupLocation="CenterScreen" Icon="Icon.ico" ResizeMode="CanMinimize">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Resource\en-us.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <BooleanToVisibilityConverter x:Key="b2v" />
        </ResourceDictionary>
    </Window.Resources>
    <Grid x:Name="MainWindowGrid">
        <StackPanel VerticalAlignment="Top" HorizontalAlignment="Right" Width="100" Panel.ZIndex="9">
            <Label x:Name="Version" Content="" Width="80" HorizontalAlignment="Left" HorizontalContentAlignment="Left"/>
            <Label>
                <Hyperlink NavigateUri="http://www.antim8.de" RequestNavigate="Hyperlink_RequestNavigate">
                    www.antim8.de
                </Hyperlink>
            </Label>
        </StackPanel>
        <StackPanel x:Name="KillSwitchEnabled" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label FontSize="50" Foreground="Red" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Sorry! The Bot isn't available any longer! #killSwitch"/>

        </StackPanel>
        <Grid x:Name="updateWindow" Visibility="Collapsed">

            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Label Content="Hey! New Update available" FontSize="70" HorizontalAlignment="Center" FontFamily="Segoe Ui Light" VerticalAlignment="Top" Margin="0,0,0,20"/>

                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <Button Content="Update" Click="updateBot" HorizontalAlignment="Center" FontSize="35" FontFamily="Segoe Ui Light" Height="100" Width="300"/>
                    <Button Content="Ask me later" Click="KeepThisVersion" Margin="20,0,0,0" FontSize="35" FontFamily="Segoe Ui Light" Height="100" Width="300"/>
                </StackPanel>
            </StackPanel>
        </Grid>
        <Grid x:Name="downloadingUpdate" Visibility="Collapsed">
            <Controls:ProgressRing Width="300" Height="300" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            <Label HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" Content="Updating" FontSize="40" Foreground="#0059b2" />
        </Grid>
        <Grid x:Name="quickStart" Visibility="Collapsed">

            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Label Content="Quick Start" FontSize="70" HorizontalAlignment="Center" FontFamily="Segoe Ui Light" VerticalAlignment="Top" Margin="0,0,0,20"/>
                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                    <Label Style="{StaticResource LineItemLabel}" Content="Auth Type" ToolTip="Google and Pokemon Trainer's Club (ptc) are the currently supported login methods." />
                    <ComboBox ItemsSource="{Binding Source={x:Type rapi:AuthType}, Converter={StaticResource EnumToArrayConv}}" SelectedItem="{Binding Settings.AuthType}" SelectionChanged="AuthType_Changed" />
                </StackPanel>
                <StackPanel Visibility="{Binding IsGoogleAuthShowing, Converter={StaticResource VisConv}}">
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label  Style="{StaticResource LineItemLabel}" Content="Google Username" ToolTip="The Google account username to use for login." />
                        <TextBox Style="{StaticResource LineItemTextBoxLg}" Text="{Binding Settings.GoogleUsername}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="Google Password" ToolTip="The Google account password to use for login." />
                        <PasswordBox x:Name="googlePasswordSetup" Width="200" model:PasswordBoxAssistant.BindPassword="True" model:PasswordBoxAssistant.BoundPassword="{Binding Settings.GooglePassword}" />
                    </StackPanel>
                </StackPanel>
                <StackPanel Visibility="{Binding IsGoogleAuthShowing, Converter={StaticResource VisRevConv}}">
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="PTC Username" ToolTip="The Pokemon Trainer's Club account username to use for login." />
                        <TextBox Style="{StaticResource LineItemTextBoxLg}" Text="{Binding Settings.PtcUsername}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="PTC Password" ToolTip="The Pokemon Trainer's Club account password to use for login." />
                        <PasswordBox x:Name="ptcPasswordSetup" Width="200" model:PasswordBoxAssistant.BindPassword="True" model:PasswordBoxAssistant.BoundPassword="{Binding Settings.PtcPassword}" />

                    </StackPanel>
                </StackPanel>
                <Button Content="Next" Click="Quick_Guide" HorizontalAlignment="Center" Margin="0,20,0,0" />
            </StackPanel>
        </Grid>
        <Grid x:Name="quickStart2" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="Collapsed">
            <StackPanel>
                <Label Content="For Google you need to define a device" FontFamily="Segoe Ui Light" FontSize="40" />
                <ScrollViewer HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Visible">
                    <StackPanel>

                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Device Package" ToolTip="The device package to use for this bot, use 'random' to choose one automatically or 'custom' to use your own configs" />
                            <ComboBox ItemsSource="{Binding DevicePackageCollection}" SelectedItem="{Binding Settings.DevicePackageName}" SelectionChanged="DevicePackage_Changed" />
                            <!--<TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DevicePackageName}" />-->
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Device ID" ToolTip="The device serial number, this is unique per device and should be a 16-digit hexadecimal value" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DeviceId}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Android Board Name" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.AndroidBoardName}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Android Bootloader" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.AndroidBootloader}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Device Brand" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DeviceBrand}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Device Model" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DeviceModel}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Device Model Identifier" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DeviceModelIdentifier}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Device Model Boot" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DeviceModelBoot}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Hardware Manufacturer" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.HardwareManufacturer}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Hardware Model" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.HardwareModel}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Firmware Brand" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.FirmwareBrand}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Firmware Tags" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.FirmwareTags}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Firmware Type" />
                            <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.FirmwareType}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                            <Label Style="{StaticResource LineItemLabel}" Content="Firmware Fingerprint" />
                            <TextBox Style="{StaticResource LineItemTextBoxLg}" Text="{Binding Settings.FirmwareFingerprint}" IsEnabled="{Binding IsCustomDevicePackage}" />
                        </StackPanel>
                    </StackPanel>
                </ScrollViewer>
                <Button Content="Next" Click="Quick_Guide" HorizontalAlignment="Center" Margin="0,20,0,0" />
            </StackPanel>
        </Grid>
        <Grid x:Name="quickStart3" Visibility="Collapsed">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                <Label Content="One Step left" HorizontalAlignment="Center" FontFamily="Segoe Ui Light" FontSize="40" />

                <StackPanel>
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="Language" Width="70"/>
                        <ComboBox x:Name="LanguageFieldSetup" Margin="170,0,0,0" Width="100" SelectedIndex="1">
                            <ComboBoxItem Content="Dutch" />
                            <ComboBoxItem Content="English"/>
                            <ComboBoxItem Content="French" />
                            <ComboBoxItem Content="German" />
                            <ComboBoxItem Content="Italian" />
                            <ComboBoxItem Content="Spanish" />
                        </ComboBox>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="Walking Speed (in Km/h)" ToolTip="How fast your character will move from point to point. For best safety we recommend keeping this at 15 or below. Decimals are allowed." />
                        <TextBox Style="{StaticResource LineItemTextBoxSm}" Text="{Binding Settings.WalkingSpeedInKilometerPerHour}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="Default Latitude" ToolTip="The default Latitude to use, if no LastPos.ini file is found will use these by default" />
                        <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DefaultLatitude}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                        <Label Style="{StaticResource LineItemLabel}" Content="Default Longitude" ToolTip="The default Longitude to use" />
                        <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DefaultLongitude}" />
                    </StackPanel>
                    <Button Content="Use Locationpicker" Click="openLocationPicker"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0,20,0,0" HorizontalAlignment="Center">
                    <Button Content="More Configs" Click="Quick_Guide" />
                    <Button Content="Start Bot" Click="startBotFromQuickGuide" Margin="20,0,0,0"/>
                </StackPanel>
            </StackPanel>
        </Grid>

        <Grid x:Name="startPage" Visibility="Visible" Width="1352" Height="670" Background="Black">
            <Image x:Name="StartScreenLogo" Width="150" Height="150" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,30,30" Panel.ZIndex="2"/>
            <Label Margin="0,0,0,0" Width="400" HorizontalAlignment="Left" Panel.ZIndex="4">
                <Hyperlink NavigateUri="http://www.antim8.de" RequestNavigate="Hyperlink_RequestNavigate">
                    <Image x:Name="Banner" Height="670" Width="400"/>
                </Hyperlink>
            </Label>
            <StackPanel HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,30,150" Panel.ZIndex="3">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Height="100">
                    <Label FontFamily="Segoe Ui Light" Foreground="White" FontSize="40" HorizontalAlignment="Center" Content="Your WeezBot is already configured" />
                    
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <Label FontFamily="segoe ui light" FontSize="30" Content="Username:" Foreground="White"/>
                    <Label x:Name="startPageUsername" FontFamily="Segoe Ui Light" FontSize="30" Content="Username" Foreground="Green" />
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Height="100" Margin="0,0,0,0">
                    <Button Content="Go To Configs" FontSize="40" Height="100" Width="400" Click="goToConfigs" />
                    <Button Content="Start Bot" FontSize="40" Height="100" Width="400" Click="button_Click" Margin="20,0,0,0"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Margin="0,20,0,0">
                    <Button Content="Want to see live updates On your Phone?" Background="Gray" Click="loginToAntim8" Foreground="White"/>
                </StackPanel>
            </StackPanel>
        </Grid>

        <StackPanel x:Name="whyUseWebsite" Visibility="Collapsed" HorizontalAlignment="Center" Margin="0,150,0,0" VerticalAlignment="Top">
            <Label Content="Reasons to Login" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="35" />
            <Label Content="- You can see your live stats (level, xp...) Online from anywhere" FontSize="25" Margin="0,10,0,0"/>
            <Label Content="Guide" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Center" FontSize="35" />
            <Label Content="1. Login here with you Antim8 Account ( Not Your Pokemon GO Account)" FontSize="25" Margin="0,10,0,0"/>
            <Label Content="2. Start the Bot" FontSize="25" Margin="0,10,0,0" />
            <Label Content="3. Go Online to www.antim8.de and Login with you Antim8 Account" FontSize="25" Margin="0,10,0,0" />
            <Label Content="4. See your Livestats" FontSize="25" Margin="0,10,0,0" />
            <Button Content="Back" HorizontalAlignment="Center" Height="50" Click="whyuseBack" FontSize="25" Margin="0,10,0,0" Width="200"/>
        </StackPanel>

        <Grid x:Name="LoginToWebsite"  Visibility="Collapsed">
            <Button HorizontalAlignment="Left" Click="backtoscreen" VerticalAlignment="Top" Margin="20,20,0,0" Height="50" FontSize="28" Content="Back To Start Screen" />
            <Button Content="Why sould I Login? " Click="whyuse" Width="150" Margin="20,90,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <StackPanel HorizontalAlignment="Center" Margin="0,0,0,100" VerticalAlignment="Center">
                <Label FontSize="40" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Content="Login to Antim8.de" Foreground="Gray" />
                <StackPanel Orientation="Horizontal" Margin="0,20,0,0" Style="{StaticResource LineItemPanel}">
                <Label Style="{StaticResource LineItemLabel}" Content="Antim8 Username" />
                <TextBox x:Name="antim8username" Style="{StaticResource LineItemTextBoxLg}" Text="{Binding Settings.antim8username}" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                <Label Style="{StaticResource LineItemLabel}" Content="Antim8 Password" />
                <PasswordBox x:Name="antim8Password" Width="200" model:PasswordBoxAssistant.BindPassword="True" model:PasswordBoxAssistant.BoundPassword="" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" Margin="0,20,0,0">
                <Button Content="Register at Antim8.de" Click="RegisterFinalle" Width="200" />
                    <Button Content="Login" Margin="40,0,0,0" Width="150" Click="LoginAntim8" />
                </StackPanel>
            <Label x:Name="LoginError" Content="" Foreground="Red" Margin="0,20,0,0" FontSize="20" />
        </StackPanel>
        </Grid>

        <StackPanel x:Name="SuccessfullyRegistered" Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" >
            <Label x:Name="SuccessMessage" Content="Your Registration is Completed" HorizontalAlignment="Center" FontSize="30" Foreground="Gray"/>
            <StackPanel Orientation="Horizontal" Margin="0,20,0,0" HorizontalAlignment="Center">
                <Button Content="Go to Configs" FontSize="20" Height="40" Width="200" Click="KeepThisVersion" Background="Gray" Foreground="White"/>
                <Button Content="Start Bot" FontSize="20" Height="40" Width="200" Background="Gray" Foreground="White" Click="startitagain_Click" Margin="20,0,0,0"/>
            </StackPanel>
        </StackPanel>
        <DockPanel x:Name="ConfigPanel" Visibility="Collapsed">
            <UniformGrid x:Name="panelUni" DockPanel.Dock="Bottom" Rows="1" Columns="4">
                <Button Margin="15" Padding="0,8" Click="DefaultsButton_Click">Defaults</Button>
                <Button Margin="15" Padding="0,8" Click="ReloadButton_Click">Reload</Button>
                <Button Margin="15" Padding="0,8" Click="SaveButton_Click">Save</Button>
                <Button x:Name="StartBotButton" Margin="15" Padding="0,8" Click="button_Click">Start Bot</Button>
            </UniformGrid>

            <TabControl SelectionChanged="TabControl_SelectionChanged">

                <TabItem Header="Basic">
                    <StackPanel Orientation="Horizontal" Width="1300">
                        <StackPanel Width="550">

                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Language" Width="70"/>
                                <ComboBox x:Name="LanguageField" Margin="170,0,0,0" Width="100" SelectedIndex="1">
                                    <ComboBoxItem Content="Dutch" />
                                    <ComboBoxItem Content="English"/>
                                    <ComboBoxItem Content="French" />
                                    <ComboBoxItem Content="German" />
                                    <ComboBoxItem Content="Italian" />
                                    <ComboBoxItem Content="Spanish" />
                                </ComboBox>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Default Latitude" ToolTip="The default Latitude to use, if no LastPos.ini file is found will use these by default" />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DefaultLatitude}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Default Longitude" ToolTip="The default Longitude to use" />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DefaultLongitude}" />
                            </StackPanel>

                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Max Spawn Location Offset" ToolTip="QED. Integer value." />
                                <TextBox Style="{StaticResource LineItemTextBoxSm}" Text="{Binding Settings.MaxSpawnLocationOffset}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Current Latitude" ToolTip="If you have run the bot before, this was the last Latitude you were at and a new start will begin here. If no current coords were found on disk, we reset this to the default values." />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.Latitude}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Current Longitude" ToolTip="If you have run the bot before, this was the last Longitude you were at and a new start will begin here. If no current coords were found on disk, we reset this to the default values." />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.Longitude}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Button Padding="8,4" Click="ResetCoordsButton_Click">Set Current Coords to Defaults</Button>
                                <Button Click="openLocationPicker" Margin="10,0,0,0">Choose Location on the Map</Button>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Fast SoftBan bypass" ToolTip="Check this if the Program should do a fast SoftBan bypass" />
                                <Controls:ToggleSwitch OffLabel="False" OnLabel="True" IsChecked="{Binding Settings.FastSoftBanBypass}"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="600">
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Disable Human Walking" ToolTip="Emulate realistic human walking from stop to stop - WARNING: Disabling this could result in getting flagged or banned!" />
                                <Controls:ToggleSwitch OffLabel="False" OnLabel="True" IsChecked="{Binding Settings.DisableHumanWalking}"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Max Travel Distance in Meters" ToolTip="The maximum distance in meters the bot will travel from the Lat/Lon being used. 1000 meters = 1 kilometer, 1600 meters = 1 mile" />
                                <TextBox Style="{StaticResource LineItemTextBoxSm}" Text="{Binding Settings.MaxTravelDistanceInMeters}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Walking Speed (in Km/h)" ToolTip="How fast your character will move from point to point. For best safety we recommend keeping this at 15 or below. Decimals are allowed." />
                                <TextBox Style="{StaticResource LineItemTextBoxSm}" Text="{Binding Settings.WalkingSpeedInKilometerPerHour}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Delay Between Player Actions (ms)" ToolTip="How long to pause between player actions in milliseconds (1000 ms = 1 second). Integer value." />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DelayBetweenPlayerActions}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Delay Between Pokemon Catch (ms)" ToolTip="How long to pause between Pokemon captures in milliseconds (1000 ms = 1 second). Integer value." />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.DelayBetweenPokemonCatch}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Bing Maps Api Key" ToolTip="If you have your own you can put it here." />
                                <TextBox Style="{StaticResource LineItemTextBoxMd}" x:Name="BingMapsApi" />
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </TabItem>

                <TabItem Header="Auth">
                    <StackPanel Orientation="Horizontal" Width="1300">
                        <StackPanel Width="600">
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Auth Type" ToolTip="Google and Pokemon Trainer's Club (ptc) are the currently supported login methods." />
                                <ComboBox ItemsSource="{Binding Source={x:Type rapi:AuthType}, Converter={StaticResource EnumToArrayConv}}" SelectedItem="{Binding Settings.AuthType}" SelectionChanged="AuthType_Changed" />
                                <Label x:Name="LoginFailedLabel" Content="Login Error" Foreground="Red" HorizontalContentAlignment="left" Visibility="Collapsed" Width="500"/>
                                <!-- ItemsSource="{Binding Source={util:EnumValues{x:Type rapi:AuthType}}}" -->
                            </StackPanel>
                            <StackPanel Visibility="{Binding IsGoogleAuthShowing, Converter={StaticResource VisConv}}">
                                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                    <Label  Style="{StaticResource LineItemLabel}" Content="Google Username" ToolTip="The Google account username to use for login." />
                                    <TextBox Style="{StaticResource LineItemTextBoxLg}" Text="{Binding Settings.GoogleUsername}" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                    <Label Style="{StaticResource LineItemLabel}" Content="Google Password" ToolTip="The Google account password to use for login." />
                                    <PasswordBox x:Name="googlePassword" Width="200" model:PasswordBoxAssistant.BindPassword="True" model:PasswordBoxAssistant.BoundPassword="{Binding Settings.GooglePassword}" />
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Visibility="{Binding IsGoogleAuthShowing, Converter={StaticResource VisRevConv}}">
                                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                    <Label Style="{StaticResource LineItemLabel}" Content="PTC Username" ToolTip="The Pokemon Trainer's Club account username to use for login." />
                                    <TextBox Style="{StaticResource LineItemTextBoxLg}" Text="{Binding Settings.PtcUsername}" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                    <Label Style="{StaticResource LineItemLabel}" Content="PTC Password" ToolTip="The Pokemon Trainer's Club account password to use for login." />
                                    <PasswordBox x:Name="ptcPassword" Width="200" model:PasswordBoxAssistant.BindPassword="True" model:PasswordBoxAssistant.BoundPassword="{Binding Settings.PtcPassword}" />
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="500">
                            <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                <Label Style="{StaticResource LineItemLabel}" Content="Enable Proxy System" ToolTip="Enable Proxy" />
                                <Controls:ToggleSwitch OffLabel="False" OnLabel="True" IsChecked="{Binding Settings.UseProxy}"/>
                            </StackPanel>
                            <StackPanel Visibility="{Binding Settings.UseProxy, Converter={StaticResource VisConv}}">
                                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                    <Label Style="{StaticResource LineItemLabel}" Content="Proxy Host" ToolTip="QED." />
                                    <TextBox Style="{StaticResource LineItemTextBoxMd}" Text="{Binding Settings.UseProxyHost}" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Style="{StaticResource LineItemPanel}">
                                    <Label Style="{StaticResource LineItemLabel}" Content="Proxy Port" ToolTip="QED." />
